{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nlt-dp203"
		},
		"ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nlt1166dp203.dfs.core.windows.net/"
		},
		"KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://nlt-dp203.vault.azure.net/"
		},
		"LS_AWS_S3_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "KeyVault",
					"type": "LinkedServiceReference"
				},
				"secretName": "AWS-S3-AssessKeyID"
			}
		},
		"LS_REST_OpenWeatherAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().base_url}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nlt-dp203_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('KeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_REST_OpenWeatherAPI_properties_typeProperties_url": {
						"value": "[parameters('LS_REST_OpenWeatherAPI_properties_typeProperties_url')]"
					},
					"ADLS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_properties_typeProperties_url')]"
					},
					"LS_AWS_S3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('LS_AWS_S3_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}